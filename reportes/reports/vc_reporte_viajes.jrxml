	<queryString language="SQL">
		<![CDATA[SELECT	Id,Inicio,Conductor, ViajesPorConductor, e.nombre_estado AS estado,km,pasajeros,precio, TotalFilas,FechaViaje, ViajesPorDia
					FROM
                      
                                         (Select count(*) as TotalFilas
											From viaje v inner join usuario u on v.id_cliente = u.id_usuario
											INNER JOIN ESTADO_VIAJE e ON v.estado = e.id_estado_viaje
											
										) as TotalFilas
					    JOIN (SELECT
					        v.id_viaje AS Id,
					        v.fecha_inicio AS Inicio,
					        v.estado AS estado,
					        
					        u.nombre_usuario AS Conductor,
					        km_por_viaje.km AS km,
					        pasajeros_por_viaje.pasajeros as pasajeros,
					        ROUND(v.Precio,2) as Precio 
					    FROM
							VIAJE v 
							INNER JOIN
								(SELECT
									v.id_viaje, ROUND(SUM(lv.kms_a_localidad_siguiente),1) as km
									FROM
										VIAJE v 
										INNER JOIN LOCALIDAD_VIAJE lv
										ON lv.id_viaje=v.id_viaje group by v.id_viaje) AS km_por_viaje
										ON v.id_viaje = km_por_viaje.id_viaje
										INNER JOIN USUARIO u  ON u.id_usuario = v.id_cliente
					       				INNER JOIN
										(SELECT v.id_viaje, COALESCE(SUM(pv.nro_asientos),0) AS pasajeros
											FROM
											VIAJE v LEFT JOIN PASAJERO_VIAJE pv
											ON pv.id_viaje=v.id_viaje GROUP BY v.id_viaje) AS pasajeros_por_viaje
											ON v.id_viaje = pasajeros_por_viaje.id_viaje) AS reporte
										INNER JOIN ESTADO_VIAJE e ON reporte.estado = e.id_estado_viaje
										   
										
                                         RIGHT JOIN 
                                         (Select count(v.id_viaje) as ViajesPorConductor, u.nombre_usuario as Cond
											From viaje v inner join usuario u on v.id_cliente = u.id_usuario INNER JOIN ESTADO_VIAJE e ON v.estado = e.id_estado_viaje	
											WHERE (
												(e.nombre_estado LIKE "$P!{estado}%")
												and (LOWER(u.nombre_usuario) = LOWER("$P!{conductor}") or "$P!{conductor}" = "")
											)																		        
											group by u.nombre_usuario) as ViajesPorConductor
                                           on ViajesPorConductor.Cond = reporte.Conductor 
                                          			
 										inner join (
									Select count(*) as ViajesPorDia, date(v.fecha_inicio) as FechaViaje
                                    from viaje v inner join usuario u on v.id_cliente = u.id_usuario INNER JOIN ESTADO_VIAJE e ON v.estado = e.id_estado_viaje	
											WHERE (
												(e.nombre_estado LIKE "$P!{estado}%")
												and (LOWER(u.nombre_usuario) = LOWER("$P!{conductor}") or "$P!{conductor}" = "")
											)                                    
                                    group by date(v.fecha_inicio)
                                ) as CantViajesPorDia
                                on date(CantViajesPorDia.FechaViaje) = date(Reporte.Inicio)
 								
 								WHERE (Id >= $P!{id_viaje_desde} or $P!{id_viaje_desde} is null)
					            and (Id <= $P!{id_viaje_hasta} or $P!{id_viaje_hasta} is null)
					            and (Inicio >= "$P!{fecha_desde}"  or "$P!{fecha_desde}" = "null")
					            and (Inicio <= "$P!{fecha_hasta}" or "$P!{fecha_hasta}" = "null")
					            and (LOWER(Conductor) = LOWER("$P!{conductor}") or "$P!{conductor}" = "")
					            and (e.nombre_estado LIKE "$P!{estado}%")
					            and (km >= $P!{km_desde} or $P!{km_desde} is null)
					            and (km <= $P!{km_hasta} or $P!{km_hasta} is null)
					            and (pasajeros >= $P!{pasajeros_desde} or $P!{pasajeros_desde} is null)
					            and (pasajeros <= $P!{pasajeros_hasta} or $P!{pasajeros_hasta} is null)
					            and (Precio >= $P!{precio_desde} or $P!{precio_desde} is null)
					            and (Precio <= $P!{precio_hasta} or $P!{precio_hasta} is null)
 
 	order by ID
 ;]]>
	</queryString>
